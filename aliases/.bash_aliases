# GIT-AUDIT: list all files in history 
alias git-audit='git log --pretty=format: --name-only --diff-filter=A  | sort -u'

# ================================
#          VSCODE/CODIUM
# ================================
alias code-extensions-dir='code --extensions-dir'
alias code-list-extensions='code --list-extensions'

